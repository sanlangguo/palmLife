<van-tabs active="{{ active }}" bind:change="onChange">
  <van-tab title="全部">
    <van-panel title="汇光皇后卤肉" desc="{{'订单号：' + item.orderNumber}}" status="{{item.active}}" wx:for="{{order}}">
      <van-card
        wx:for="{{item.goods}}"
        num="{{item.num}}"
        price="{{item.price}}"
        desc="{{item.desc}}"
        title="商品标题"
        thumb="{{ 'https://6465-dev-o45qm-1301624908.tcb.qcloud.la/goods-list/10141584450128_.pic.jpg' }}"
      />
    </van-panel>
  </van-tab>
  <van-tab title="待发货">待发货</van-tab>
  <van-tab title="已收货">已收货</van-tab>
</van-tabs>
